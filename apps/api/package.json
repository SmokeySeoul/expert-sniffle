{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "tsx src/index.ts",
    "worker": "tsx src/worker.ts",
    "dev:all": "concurrently -k \"npm run dev\" \"npm run worker\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "bullmq": "^5.31.6",
    "fastify": "^4.27.2",
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "concurrently": "^8.2.2",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
